---
eleventyComputed:
  title: "{{ categoriasProductos[0].titleseo }}"
  description: "{{ categoriasProductos[0].descseo }}"
  keywords: "{{ categoriasProductos[0].keyseo }}"
layout: base.njk
pagination:
  data: collections.categorias
  size: 1
  alias: categoria
permalink: "/desarrollos/{{ categoria.slug.current }}/"
---

<div
  data-barba="container"
  data-barba-namespace="categoria-producto"
  data-css="productos-category" 
  data-js="productos">
  
  <section id="section__hero">
  </section>

  <section id="section__intro" class="pad__all__5">
    <div class="column__1">
      <h1>{{ categoria.slug.current }}</h1>
      <ul role="list" class="listado__x3 width__100">
        {% for item in categoria.products %}
        {% include "producto-card.njk" %}
        {% endfor %}
      </ul>
    </div>
  </section>
</div>
